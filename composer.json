{
    "name": "stechstudio/laravel-beankeep",
    "description": "A Laravel wrapper for stechstudio/beankeep",
    "type": "library",
    "version": "0.1.0-dev",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "STS\\Beankeep\\": "src/"
        }
    },
    "authors": [
        {
            "name": "V13Axel",
            "email": "alex@stechstudio.com"
        },
        {
            "name": "Zeb DeOs",
            "email": "zebulin@stechstudio.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0",
        "illuminate/database": "^10.0",
        "nunomaduro/collision": "^7.7"
    },

    "extra": {
        "laravel": {
            "providers": [
                "STS\\Beankeep\\BeankeepServiceProvider"
            ],
            "aliases": {}
        }
    },
    "require-dev": {
        "orchestra/testbench": "^8.5",
        "phpunit/phpunit": "^10.2"
    },
    "autoload-dev": {
        "psr-4": {
            "STS\\Beankeep\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php vendor/bin/testbench package:discover --ansi"
        ],
        "test": [
            "@putenv COLLISION_PRINTER=DefaultPrinter",
            "@php vendor/bin/phpunit --no-output"
        ]
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
